{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/data-fixtures": "1.8.1",
        "doctrine/doctrine-bundle": "^2.8",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "3.3.3",
        "guzzlehttp/guzzle": "^7.5",
        "scienta/doctrine-json-functions": "*",
        "symfony/cache": "*",
        "symfony/console": "*",
        "symfony/doctrine-messenger": "*",
        "symfony/dotenv": "*",
        "symfony/finder": "*",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "*",
        "symfony/http-client": "*",
        "symfony/lock": "*",
        "symfony/messenger": "*",
        "symfony/monolog-bundle": "^3.8",
        "symfony/rate-limiter": "*",
        "symfony/runtime": "*",
        "symfony/serializer": "*",
        "symfony/uid": "*",
        "symfony/validator": "*",
        "symfony/yaml": "*"
    },
    "require-dev": {
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^9.5",
        "rector/rector": "*",
        "symfony/browser-kit": "*",
        "symfony/css-selector": "*",
        "symfony/maker-bundle": "^1.48",
        "symfony/phpunit-bridge": "^6.2",
        "symfony/process": "*"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "App\\Bot\\": "src/modules/Bot",
            "App\\Alarm\\": "src/modules/Alarm",
            "App\\Output\\": "src/modules/Output",
            "App\\Connection\\": "src/modules/Connection",
            "App\\Settings\\": "src/modules/Settings",
            "App\\Liquidation\\": "src/modules/Liquidation",
            "App\\Service\\": "src/modules/Service",
            "App\\Trading\\": "src/modules/Trading",
            "App\\Stop\\": "src/modules/Stop",
            "App\\Buy\\": "src/modules/Buy",
            "App\\Profiling\\": "src/modules/Profiling",

            "Lin\\Bybit\\": "lib/LinByBit/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.4.*"
        }
    }
}
